                        android:paddingTop="10dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="5dp"
                        android:layout_height="50dp"
                        android:textColor="#FFFFFF"
                        android:textAlignment="center"
                        android:textSize="30dp"
                        android:textStyle="bold"
